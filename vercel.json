{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "dist"  // Ganti jika output Anda di folder lain, misalnya 'dist'
      }
    }
  ],

  "rewrites": [
      {"source": "/(.*", "destination": "/"}
    ]

  }
  